git init 
git add .
git commit -m "initial commit"
git remote add origin <link repo>
 git branch -M main
 git pull origin main --rebase  # force pull from git hub
 git push origin main

git remote -v   # check remote repo

git add -A    # Add changes to git

git config --global user.name "Your Name"
git config --global user.email "you@example.com"

git clone <repo-url>          # Clone a repo


git branch                    # List all branches
git branch <name>             # Create a new branch
git switch <branch-name>      # Switch to another branch
git checkout <branch-name>    # (older alternative to switch)
git merge <branch-name>       # Merge another branch into current
git branch -d <branch-name>   # Delete a branch


git rm <name>             # Delete file
git rm --cached -r <file> #  This does not delete the file from disk â€” it just removes it from Git tracking.



git log                       # View commit history
git log --oneline --graph     # Compact view with branch graph
git show <commit-hash>        # Show details of a commit


git tag                       # List all tags
git tag <tag-name>            # Create a tag
git push origin <tag-name>    # Push a tag to remote


pip install git-filter-repo
git filter-repo --force --path <file to remove from git tracking> --invert-paths # remove from git tracking after it is commited


